<h1><%= t :degrees %></h1>

<ul>
  <% @degrees.each do |degree| %>
    <li><%= degree %> (<%= degree.code %>) <%= link_to icon('pencil'), edit_admin_degree_path(degree) %></li>
  <% end %>
</ul>

